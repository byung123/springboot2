<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.rest.repository.ColorMapper"> <!-- repository있는걸 오버라이드 해줘야한다 -->
    <!-- id가 method, resultType Entity로 잡아주면 알아서 같은 자료형으로 리턴해줌 -->
    <select id="findAll" resultType="com.study.rest.entity.Color">
        select
            colorId,
            colorName
        from
            color_tb
        order by
            colorId
    </select>
</mapper>